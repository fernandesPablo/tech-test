{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft.AspNetCore": "Error"
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Microsoft": "Error",
                "System": "Error"
            }
        }
    },
    "CsvBackup": {
        "BackupIntervalMinutes": 60,
        "MaxBackups": 24
    },
    "RateLimiting": {
        "PermitLimit": 50,
        "WindowMinutes": 1,
        "QueueLimit": 5,
        "RetryAfterSeconds": 60
    }
}